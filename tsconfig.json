{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strictNullChecks": true,
        "module": "esnext",
        "jsx": "react-jsx",
        "target": "es6",
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "resolveJsonModule": true,
    },
    "include": ["src"]
}
